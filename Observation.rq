#+ summary: Returns all predicates and subjects related to an Observation
#+          instance.
#+ tags:
#+   - sensor-things

PREFIX sosa: <http://www.w3.org/ns/sosa/>
PREFIX wosis_obs: <http://wosis.isric.org/observation#>

SELECT ?predicate ?object ?something
WHERE {
    ?_id a sosa:Observation ;
             ?predicate ?object .
    BIND (CONCAT("something ", ?_id) AS ?something)
}
